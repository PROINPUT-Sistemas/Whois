{
  "name": "pentagonal/whois",
  "description": "Domain & Network Whois checker & info",
  "type": "project",
  "require": {
    "php": ">= 7",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^6",
    "true/punycode": "^2.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "phpunit/phpunit": "^6",
    "satooshi/php-coveralls": "^1"
  },
  "suggest" : {
    "ext-intl": "To get function idn_to_utf8 or idn_to_ascii more dast and not use puny external library",
    "ext-iconv": "To sanitize non ascii string to display on result"
  },
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "pentagonal",
      "email": "org@pentagonal.org"
    }
  ],
  "autoload" : {
    "psr-4" : {
      "\\Pentagonal\\WhoIs\\App\\": "Src/App/",
      "\\Pentagonal\\WhoIs\\Exceptions\\": "Src/Exceptions/",
      "\\Pentagonal\\WhoIs\\Handler\\": "Src/Handler/",
      "\\Pentagonal\\WhoIs\\Interfaces\\": "Src/Interfaces/",
      "\\Pentagonal\\WhoIs\\Util\\": "Src/Util/"
    },
    "classmap": [
      "Src/App",
      "Src/Exceptions",
      "Src/Handler",
      "Src/Interfaces",
      "Src/Util"
    ]
  }
}
